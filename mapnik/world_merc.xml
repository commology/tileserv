<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map srs="+proj=merc +lon_0=0 +lat_ts=0 +x_0=0 +y_0=0 +datum=WGS84 +units=m +no_defs ">
  <Style name="My Style">
    <Rule>
      <PolygonSymbolizer fill="#f2eff9" opacity="0.5" />
      <LineSymbolizer stroke="rgb(50%,50%,100%)" stroke-width="0.5" />
    </Rule>
  </Style>
  <Layer name="world" srs="+proj=merc +lon_0=0 +lat_ts=0 +x_0=0 +y_0=0 +datum=WGS84 +units=m +no_defs ">
    <StyleName>My Style</StyleName>
    <Datasource>
      <Parameter name="type">shape</Parameter>
      <Parameter name="file">../data/world_merc.shp</Parameter>
    </Datasource>
  </Layer>
</Map>
